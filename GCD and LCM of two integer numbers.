#include <stdio.h>

int GCD(int m, int n);

int main() {
    int m, n, gc, lc;
    printf("Enter two numbers (m & n): ");
    scanf("%d %d", &m, &n);

    gc = GCD(m, n);
    lc = (m * n) / gc;

    printf("GCD(%d, %d) = %d\n", m, n, gc);
    printf("LCM(%d, %d) = %d\n", m, n, lc);

    return 0;
}

int GCD(int m, int n) {
    if (n > m)
        return GCD(n, m);
    else if (n == 0)
        return m;
    else
        return GCD(n, m % n);
}
